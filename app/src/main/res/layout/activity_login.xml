<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sanukumar.billsplit.login">

    <com.google.android.gms.common.SignInButton
        android:id="@+id/login11"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/facebook"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:tooltipText="SIGN IN WITH GOOGLE"


        tools:ignore="UnknownId"></com.google.android.gms.common.SignInButton>


</RelativeLayout>
